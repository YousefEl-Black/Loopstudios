header(class="d-flex")
  .logo(class="col-7")
    img(src="imgs/logo.svg" alt="Loopstudios Logo")
  -var vals = ["about",   "careers", "events", "products", "support"]
  ul.links(class="d-none d-lg-flex justify-content-between col-5")
    each val in vals 
      li
        a(class="text-white text-decoration-none" href="#" + val)= val
  .hamburger(class="d-flex d-lg-none col-5 justify-content-end")
    img(src="imgs/icon-hamburger.svg", alt="Hamburger Icon")
  .megamenu(class="position-fixed pt-3")
    .container
      .top(class="d-flex justify-content-between align-items-center")
        .logo
          img(src="imgs/logo.svg", alt="Loopstudios Logo")
        .close
          img(src="imgs/icon-close.svg", alt="Close Icon")
      .links
        ul
          each val in vals
            li
              a(class="text-white text-decoration-none")= val
